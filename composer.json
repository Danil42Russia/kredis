{
  "name": "danil42russia/kredis",
  "description": "",
  "license": "MIT",
  "type": "library",
  "keywords": [
    "redis",
    "kphp"
  ],
  "require": {
    "php": "^7.4 || ^8.0"
  },
  "require-dev": {
    "ext-redis": "*",
    "ergebnis/composer-normalize": "^2.28",
    "phpunit/phpunit": "^9.5"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "Kredis\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    },
    "classmap": [
      "tests/"
    ],
    "files": [
      "src/helper.php"
    ]
  },
  "config": {
    "allow-plugins": {
      "ergebnis/composer-normalize": true
    },
    "optimize-autoloader": true,
    "sort-packages": true
  },
  "scripts": {
    "test:unit": "vendor/bin/phpunit --configuration phpunit.xml"
  }
}
